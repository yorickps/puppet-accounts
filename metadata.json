{
  "name": "deric-accounts",
  "version": "2.1.0-unreleased",
  "author": "Tomas Barton",
  "summary": "Module for managing user accounts and groups and SSH keys.",
  "license": "Apache-2.0",
  "source": "https://github.com/deric/puppet-accounts",
  "project_page": "https://github.com/deric/puppet-accounts",
  "issues_url": "https://github.com/deric/puppet-accounts/issues",
  "description": "User and groups management with hiera support.",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.6.0 < 9.0.0"
    },
    { "name": "puppetlabs-augeas_core",
      "version_requirement": "> 1.0.0"
    },
    { "name": "puppetlabs-sshkeys_core",
      "version_requirement": "> 1.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
